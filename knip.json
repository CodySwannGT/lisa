{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/**/*.ts"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/test/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "jscodeshift",
    "commitizen",
    "eslint-*",
    "lint-staged",
    "@commitlint/*"
  ],
  "ignoreBinaries": ["expo", "playwright", "audit"],
  "ignoreExportsUsedInFile": true
}
