{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/e2e/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/ios/**",
    "**/android/**",
    "**/.expo/**",
    "**/assets/**",
    "**/scripts/**",
    "**/eslint-plugin-*/**",
    "**/generated/**",
    "**/patches/**",
    "**/projects/**",
    "**/specs/**",
    "**/docs/**",
    "**/*.config.js",
    "**/*.config.ts",
    "**/metro.config.js",
    "**/babel.config.js"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "rules": {
    "dependencies": "off",
    "devDependencies": "off",
    "unlisted": "off",
    "binaries": "off",
    "unresolved": "off",
    "files": "off",
    "duplicates": "off",
    "exports": "off",
    "types": "off"
  }
}
