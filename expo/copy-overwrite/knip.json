{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/e2e/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/ios/**",
    "**/android/**",
    "**/.expo/**",
    "**/assets/**",
    "**/scripts/**",
    "**/eslint-plugin-*/**",
    "**/generated/**",
    "**/patches/**",
    "**/projects/**",
    "**/specs/**",
    "**/docs/**",
    "**/*.config.js",
    "**/*.config.ts",
    "**/metro.config.js",
    "**/babel.config.js"
  ],
  "ignoreBinaries": ["audit", "ast-grep", "maestro", "eas", "source-map-explorer"],
  "ignoreDependencies": [
    "@react-native-community/netinfo",
    "aws-exports",
    "@babel/core",
    "@babel/plugin-proposal-export-namespace-from",
    "@commitlint/cli",
    "@dnd-kit/modifiers",
    "@eslint-community/eslint-plugin-eslint-comments",
    "@eslint/eslintrc",
    "@eslint/js",
    "@expo-google-fonts/inter",
    "@jest/test-sequencer",
    "@react-native-community/cli",
    "@react-native-community/cli-platform-android",
    "@react-native-community/cli-platform-ios",
    "@react-navigation/native",
    "@types/eslint-plugin-jsx-a11y",
    "@types/eslint-plugin-tailwindcss",
    "@types/fs-extra",
    "@types/glob",
    "@types/lodash.merge",
    "babel-plugin-istanbul",
    "baseline-browser-mapping",
    "date-fns-tz",
    "eslint-config-expo",
    "eslint-config-prettier",
    "eslint-plugin-code-organization",
    "eslint-plugin-component-structure",
    "eslint-plugin-functional",
    "eslint-plugin-jsdoc",
    "eslint-plugin-jsx-a11y",
    "eslint-plugin-prettier",
    "eslint-plugin-react",
    "eslint-plugin-react-compiler",
    "eslint-plugin-react-hooks",
    "eslint-plugin-react-perf",
    "eslint-plugin-sonarjs",
    "eslint-plugin-tailwindcss",
    "eslint-plugin-ui-standards",
    "events",
    "expo-atlas",
    "expo-clipboard",
    "expo-localization",
    "expo-status-bar",
    "firebase",
    "glob",
    "globals",
    "i18n-js",
    "jscodeshift",
    "lint-staged",
    "memfs",
    "prettier-plugin-tailwindcss",
    "react-native-element-dropdown",
    "tailwind-variants",
    "tar",
    "text-encoding-polyfill",
    "ts-jest",
    "ts-morph",
    "typescript-eslint",
    "usehooks-ts",
    "zod"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "rules": {
    "dependencies": "error",
    "devDependencies": "error",
    "unlisted": "error",
    "binaries": "error",
    "unresolved": "error",
    "files": "error",
    "duplicates": "error",
    "exports": "error",
    "types": "error"
  }
}
