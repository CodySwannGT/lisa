{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "services/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "contexts/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "index.ts",
    "App.tsx"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "services/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "contexts/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/e2e/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/ios/**",
    "**/android/**",
    "**/.expo/**",
    "**/assets/**",
    "**/scripts/**",
    "**/eslint-plugin-*/**"
  ],
  "ignoreDependencies": [
    "jscodeshift",
    "ts-morph",
    "commitizen",
    "eslint-*",
    "lint-staged",
    "ts-node",
    "@commitlint/*",
    "expo-*",
    "@expo/*",
    "react-native",
    "react-native-*",
    "@react-native/*",
    "@react-navigation/*"
  ],
  "ignoreExportsUsedInFile": true
}
