{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "stores/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "config/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/e2e/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/ios/**",
    "**/android/**",
    "**/.expo/**",
    "**/assets/**",
    "**/scripts/**",
    "**/eslint-plugin-*/**",
    "**/generated/**",
    "**/patches/**",
    "**/projects/**",
    "**/specs/**",
    "**/docs/**",
    "**/*.config.js",
    "**/*.config.ts",
    "**/metro.config.js",
    "**/babel.config.js"
  ],
  "ignoreBinaries": ["audit", "ast-grep", "maestro", "eas", "source-map-explorer"],
  "ignoreDependencies": [
    "@dnd-kit/modifiers",
    "@expo-google-fonts/inter",
    "@gorhom/bottom-sheet",
    "@hookform/resolvers",
    "@react-native-async-storage/async-storage",
    "@react-native-community/cli",
    "@react-native-community/cli-platform-android",
    "@react-native-community/cli-platform-ios",
    "@react-native-community/netinfo",
    "@react-native-masked-view/masked-view",
    "@react-navigation/elements",
    "@react-navigation/native",
    "@shopify/flash-list",
    "@shopify/react-native-skia",
    "aws-exports",
    "base-64",
    "baseline-browser-mapping",
    "date-fns",
    "date-fns-tz",
    "events",
    "expo-application",
    "expo-battery",
    "expo-clipboard",
    "expo-crypto",
    "expo-device",
    "expo-linear-gradient",
    "expo-localization",
    "expo-network",
    "expo-notifications",
    "expo-status-bar",
    "firebase",
    "i18n-js",
    "lucide-react-native",
    "patch-package",
    "react-hook-form",
    "react-native-element-dropdown",
    "react-native-keyboard-controller",
    "react-native-store-version",
    "tailwind-variants",
    "tar",
    "text-encoding-polyfill",
    "use-debounce",
    "usehooks-ts",
    "zod"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "rules": {
    "dependencies": "error",
    "devDependencies": "off",
    "unlisted": "error",
    "binaries": "error",
    "unresolved": "error",
    "files": "error",
    "duplicates": "error",
    "exports": "error",
    "types": "error"
  }
}
