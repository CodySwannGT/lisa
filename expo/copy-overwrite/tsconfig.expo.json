{
  "extends": ["expo/tsconfig.base", "./tsconfig.base.json"],
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": "./",
    "noEmit": true,
    "declaration": false,
    "allowImportingTsExtensions": true,
    "paths": {
      "@/graphql/*": ["./generated/*"],
      "@/*": ["./*"]
    },
    "moduleSuffixes": [".ios", ".android", ".native", ".web", ""]
  }
}
