{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/**/*.ts"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests/**",
    "**/test/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "jscodeshift",
    "eslint-*",
    "lint-staged",
    "@commitlint/*",
    "@as-integrations/express5",
    "@aws-sdk/client-apigatewaymanagementapi",
    "@nestjs/terminus",
    "aws-xray-sdk-core",
    "graphql-query-complexity",
    "graphql-subscriptions",
    "graphql-ws",
    "@aws-sdk/client-appconfig",
    "@aws-sdk/client-cloudwatch",
    "@aws-sdk/client-dynamodb",
    "@aws-sdk/client-lambda",
    "@aws-sdk/lib-dynamodb",
    "@nestjs/axios",
    "@sentry/profiling-node",
    "csv-parse",
    "graphql-scalars",
    "@graphql-codegen/*",
    "@nestjs/cli",
    "@istanbuljs/nyc-config-typescript",
    "@jest/test-sequencer",
    "serverless-*",
    "@types/fs-extra",
    "@types/lodash.merge",
    "@vitest/coverage-v8",
    "memfs",
    "vitest",
    "@types/graphql-type-json",
    "@types/snowflake-sdk",
    "dotenv-cli",
    "esbuild",
    "esbuild-plugin-tsc",
    "npm-check",
    "source-map-support",
    "ts-loader",
    "tslint",
    "lodash",
    "cache-manager",
    "@jest/globals"
  ],
  "ignoreBinaries": ["expo", "playwright", "audit", "docker", "eval"],
  "ignoreExportsUsedInFile": true
}
